<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.tg.entity">
   <class name="Scenery" table="scenery">
       <id name="viewNo" column="viewno" type="java.lang.String">
           <generator class="assigned" />
       </id>
       <property name="name"  column="name" type="java.lang.String"/>
       <property name="number"  column="number" type="java.lang.Integer"/>
       <property name="score"  column="score" type="java.lang.Integer"/>
       <property name="nature"  column="nature" type="java.lang.Integer"/>
       <property name="humanLandscape"  column="hl" type="java.lang.Integer"/>
       <property name="folkCultrue"  column="fc" type="java.lang.Integer"/>
       <property name="catering"  column="catering" type="java.lang.Integer"/>
       <property name="historyCultrue"  column="hc" type="java.lang.Integer"/>
       <property name="literatureArt"  column="la" type="java.lang.Integer"/>
       <property name="countryside"  column="countryside" type="java.lang.Integer"/>
       <property name="urbanModern"  column="um" type="java.lang.Integer"/>
   </class>
</hibernate-mapping>